{
    "pipeline":[
        {
          "type":"readers.text",
          "filename":"20??_Antarctica_TO*.csv",
          "separator":",",
          "skip":1,
          "header":"Y,X,TIME,THICK,ELEVATION,FRAME,SURFACE,BOTTOM,QUALITY",
          "usecols":"X,Y,ELEVATION,BOTTOM",
          "converters": {"Z": "ELEVATION-BOTTOM"},
          "dropcols":"ELEVATION,BOTTOM"
        },
        {
          "type":"filters.reprojection",
          "in_srs":"EPSG:4326",
          "out_srs":"EPSG:3031"
        }
    ]
}